@article{Quinn:1997vn,
author = {Quinn, Thomas and Katz, Neal and Stadel, Joachim and Lake, George},
title = {{Time stepping N-body simulations}},
journal = {arXiv.org},
year = {1997},
eprint = {astro-ph/9710043v1},
eprinttype = {arxiv},
eprintclass = {astro-ph},
month = oct,
annote = {16 pages, 12 figures, submitted to Ap. J},
read = {Yes},
rating = {0},
date-added = {2011-03-09T10:01:53GMT},
date-modified = {2011-11-22T11:35:52GMT},
abstract = {Leapfrog integration has been the method of choice in N-body simulations owing to its low computational cost for a symplectic integrator with second order accuracy. We introduce a new leapfrog integrator that allows for variable timesteps for each particle in large N-body simulations. Tests with single particles in fixed potentials show that it behaves as a symplectic integrator. We then examine the results of both standard leapfrog and our temporally adaptive leapfrog on full N-body integrations of clusters and large scale structure establishing accuracy criteria for both methods. The adaptive method shows significant speed-ups over single step integrations---but the integrator no longer appears to be symplectic or, in the case of large scale structure simulations, accurate. This loss of accuracy appears to be caused by the way that the timestep is chosen, not by the integrator itself. We present a related integration technique that does retain sufficient accuracy. Although it is not symplectic, it is apparently better than previous implementations and is our current integrator of choice for large astrophysical simulations. We also note that the standard leapfrog difference equations used in cosmological N-body integrations in comoving coordinates are not symplectic. We derive an implementation of leapfrog that is in comoving canonical coordinates to correct for this deficiency.},
url = {http://arxiv.org/abs/astro-ph/9710043v1},
local-url = {file://localhost/Users/jj374/Light%20Reading/Papers2-2011/1997/Quinn/arXiv%201997%20Quinn.pdf},
file = {{arXiv 1997 Quinn.pdf:/Users/jj374/Light Reading/Papers2-2011/1997/Quinn/arXiv 1997 Quinn.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/2EC75C4F-93C0-4BEB-9363-4404D9367764}}
}

